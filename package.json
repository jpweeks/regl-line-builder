{
  "name": "regl-line-builder",
  "version": "0.1.0",
  "description": "Draw pretty lines in WebGL with the Canvas2D API.",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "scripts": {
    "start": "sh ./scripts/start.sh",
    "build": "npm run build-dist && npm run build-minify",
    "build-dist": "sh ./scripts/build-dist.sh",
    "build-test": "sh ./scripts/build-dist.sh --watch",
    "build-minify": "sh ./scripts/build-minify.sh",
    "test": "npm run test-lint && npm run test-unit",
    "test-lint": "sh ./scripts/test-lint.sh",
    "test-unit": "sh ./scripts/test-unit.sh"
  },
  "author": "Jay Weeks <jay.patrick.weeks@gmail.com> (https://jayweeks.com)",
  "license": "MIT",
  "keywords": [
    "webgl",
    "regl"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/jpweeks/regl-line-builder.git"
  },
  "devDependencies": {
    "@jpweeks/rollupify": "^0.4.0",
    "@jpweeks/rstats": "^1.2.0",
    "@shotamatsuda/rollup-plugin-glslify": "^0.1.0",
    "budo": "^10.0.4",
    "eslint": "^4.6.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "gl": "^4.0.4",
    "regl": "^1.3.0",
    "rollup": "^0.50.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0",
    "uglify-es": "^3.1.3"
  },
  "dependencies": {
    "gl-matrix": "^2.4.0"
  }
}
